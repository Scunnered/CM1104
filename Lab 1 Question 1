	public static void main(String[] args) {
        	char a = 11;
        	char b = 10;
        
       		System.out.println(a + b);
                	System.out.println(a * b);
                        	System.out.println(a - b);
                                	System.out.println(a / b);
                                            System.out.println(a % b);
    
    }
